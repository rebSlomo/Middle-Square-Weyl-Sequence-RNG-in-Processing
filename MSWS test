// demonstrates how to set up two independent MSWS RNG using the randomizer class
// different seeds maybe used for each

void setup()
{
  randomizer r1,r2;
  r1 = new randomizer();
  println("Default r1 seed: ",r1.print64(r1.s));
  r1.seed(1964,1022);
  println("New r1 seed: ",r1.print64(r1.s));
  
  r2 = new randomizer();
  println("Default r2 seed: ",r2.print64(r2.s),"\n");
  
  for(int i=0; i<10; i++)
  {
    println(r1.msws(),"   ",r2.msws());
  }
  exit();
}
